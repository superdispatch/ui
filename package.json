    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.0",
    "react-element-to-jsx-string": "^14.3.1",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "snapshot-diff": "^0.8.0",
    "storybook-addon-playroom": "^1.3.0",
    "typescript": "~3.9.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
