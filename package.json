{
  "name": "@superdispatch/ui",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/superdispatch/ui.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build --no-private",
    "prebump": "yarn && yarn lint && yarn docs && yarn test && yarn build",
    "bump": "lerna version",
    "predocs": "rimraf docs",
    "docs": "run-s storybook:build playroom:build ",
    "lint": "js-tools lint",
    "preplayroom:build": "yarn playroom:generate",
    "playroom:build": "playroom build",
    "playroom:generate": "node scripts/generate-playroom-components.js",
    "preplayroom:start": "yarn playroom:generate",
    "playroom:start": "playroom start",
    "prestart": "yarn",
    "start": "run-p storybook:start playroom:start",
    "storybook:build": "build-storybook -o docs --quiet",
    "storybook:start": "start-storybook -p 5000 --ci",
    "tdd": "jest --watch",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "prettier": "@superdispatch/prettier-config",
  "devDependencies": {
    "@codemod/cli": "^2.2.1",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@mdi/js": "^5.5.55",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-bundle-types": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@storybook/addon-actions": "^6.0.0",
    "@storybook/addon-docs": "^6.0.0",
    "@storybook/addon-essentials": "^6.0.0",
    "@storybook/addon-knobs": "^6.0.0",
    "@storybook/react": "^6.0.0",
    "@storybook/source-loader": "^6.0.0",
    "@superdispatch/babel-preset": "^0.1.57",
    "@superdispatch/dates": "^0.3.21",
    "@superdispatch/eslint-plugin": "^0.1.60",
    "@superdispatch/js-tools": "^0.1.60",
    "@superdispatch/phones": "^0.3.21",
    "@superdispatch/prettier-config": "^0.1.26",
    "@superdispatch/tsconfig": "^0.1.13",
    "@superdispatch/ui": "^0.3.21",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.11",
    "@types/css": "^0.0.31",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/luxon": "^1.24.0",
    "@types/mockdate": "^2.0.0",
    "@types/prettier": "^2.0.1",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-window": "^1.8.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-direct-import": "^0.6.2",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "css": "^3.0.0",
    "dedent": "^0.7.0",
    "eslint": "^7.2.0",
    "execa": "^4.0.3",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "jest": "^26.3.0",
    "jest-watch-typeahead": "^0.6.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.10",
    "lodash": "^4.17.15",
    "lorem-ipsum": "^2.0.3",
    "mockdate": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "playroom": "^0.21.2",
    "prettier": "^2.0.5",
