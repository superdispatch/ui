{
  "name": "@superdispatch/ui",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/superdispatch/ui.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build --no-private",
    "prebump": "yarn && yarn lint && yarn docs && yarn test && yarn build",
    "bump": "lerna version",
    "docs": "yarn workspace @superdispatch/ui-webpage build",
    "lint": "js-tools lint",
    "prestart": "yarn",
    "start": "yarn workspace @superdispatch/ui-webpage start",
    "tdd": "jest --watch",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "prettier": "@superdispatch/prettier-config",
  "devDependencies": {
    "@material-ui/core": "^4.9.12",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-bundle-types": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@superdispatch/eslint-plugin": "^0.1.54",
    "@superdispatch/js-tools": "^0.1.46",
    "@superdispatch/prettier-config": "^0.1.26",
    "@superdispatch/tsconfig": "^0.1.48",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.3",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.1.0",
    "@types/css": "^0.0.31",
    "@types/faker": "^4.1.10",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/mockdate": "^2.0.0",
    "@types/prettier": "^2.0.0",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-window": "^1.8.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "css": "^2.2.4",
    "eslint": "^6.8.0",
    "firebase-tools": "^8.0.1",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "^10.1.7",
    "lodash": "^4.17.15",
    "mockdate": "^2.0.5",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "snapshot-diff": "^0.7.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
